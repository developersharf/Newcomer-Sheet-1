#include <stdio.h>
#include <math.h>
int main()
{
    double n, a;
    scanf("%lf%lf", &n, &a);
    
    printf("floor %.0lf / %.0lf = %.0lf\n", n, a, floor(n/a));
    printf("ceil %.0lf / %.0lf = %.0lf\n", n, a, ceil(n/a));
     printf("round %.0lf / %.0lf = %.0lf", n, a, round(n/a));
    
    return 0;
}
